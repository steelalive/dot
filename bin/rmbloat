for dir in '/' /data/app /system/app /system/priv-app; do
	cd "$dir"
	for crap in $DELETE
		do full=*"${dir}/${crap}"*
		[[ -e "$full" ]] && rm -rfv "${full}"* && ANRED "UNINSTALLED ${Y}$full${R} ...\n"
		rm -rfv "${dir}/${crap}"* && ANRED "UNINSTALLED ${Y}${dir}/${crap}${R} ...\n"
	done
done
