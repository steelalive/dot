#!/bin/bash
#-*- coding: utf-8 -*-
#2#::.. Last edit: - Fri Sep 30 16:33:20 EDT 2016 - by: - __INDNYALL__ - ..::## #_# - VERSION=0.0.0.6 - #_# #@#300916#@# #2#
#3#::..#####################_MAIN_#######################..::#3#
# Author: __INDNYALL__#
aquote() {
	goal="$(echo $1 | sed 's/^/${/' | sed 's/\>/[@]}/')"
	echo "\"${goal}\""
}
aquote "$@"
