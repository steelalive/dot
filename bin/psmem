#3#::..#####################_/dot/bin/psmem_#######################..::#3#
echo 'USER       PID %CPU %MEM    VSZ   RSS TTY      STAT START   TIME COMMAND'
unbuffer /usr/bin/grc -es --colour=auto ps aux --sort=%mem | tail -n20
