[[ $1 ]] || exit 2
[[ $1 == "R" ]] && shift
[[ $1 == "r" ]] && shift
[[ $1 == "-R" ]] && shift
[[ $1 == "-r" ]] && shift
[[ -e $1 ]] || exit
+user "$@"
+777 "$@"
fixdot fix
