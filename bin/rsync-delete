#!/bin/bash
#-*- coding: utf-8 -*-
#1#_SCRIPT_#1# - by: steelalive
#2#::.. Last edit: - Tue Sep 10 06:53:25 EDT 2019 ..::## #_# - VERSION=0.0.0.1 - #_# #@#2019-09-10#@# #2#
# vi: set noro: ft=sh
#3#::..#####################_/dot/bin/rsync-delete_#######################..::#3#
mkdir empty && rsync -r --delete empty/ "$@" && rmdir empty


