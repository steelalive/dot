#!/bin/bash
#-*- coding: utf-8 -*-
#1#_SCRIPT_#1# - by: steelalive
#2#::.. Last edit: - Mon Jun 25 14:51:18 EDT 2018 ..::## #_# - VERSION=0.0.0.1 - #_# #@#2018-06-25#@# #2#
# vi: set noro: ft=sh
#3#::..#####################_/dot/bin/unzipl_#######################..::#3#
unzipl() {
	[[ "$1" ]] && fname="$1"
	[[ "$1" ]] || fname="$1"
	unzip -l "$1"
	fname="$(sed "s|.zip||" <<<"$fname")"
	echo "Extract to /prog/unzip/$fname"
	if yorn y; then
		unzip "$@" -d /prog/unzip/"$fname"
	else
		read -p "Enter path 
    result:   path/$fname
    ?" fpath
		unzip "$@" -d "$fpath/$fname"
	fi
}
unzipl "$@"
